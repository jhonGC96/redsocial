<%- include ("cabeceras/cabecera.principal.ejs") %>
    <div class="container p-4">
        <div class="row">
            <div class="col-md-12">
                <div class="table-wrapper col-md-12">
                    <div class="table-title col-md-12">
                        <div>
                            <h2>Valora tus
                                <%= nombretabla %> del 1-10
                            </h2>
                        </div>
                    </div>
                    <form id='tablaAlta' Method="post">
                        <table class="table table-striped table-hover col-md-12">
                            <thead class="thead-dark">
                                <tr>
                                    <th>No</th>
                                    <th>
                                        <%= nombretabla %>
                                    </th>
                                    <th>Antes de Tecla</th>
                                    <th>Despues de Tecla</th>
                                </tr>
                            </thead>

                            <tbody>
                                <tr>
                                    <th><input id="row1" value="<%= data[0].id_conocimiento %>" type="number" class="form-control" disabled></th>
                                    <th><input id="row2" type="text" value="<%= data[0].descripcion_conocimiento %>" class="form-control" disabled></th>
                                    <th><input id="row3" type="number" min="1" max="10" class="form-control" required></th>
                                    <th><input id="row4" type="number" min="1" max="10" class="form-control" required></th>
                                </tr>
                                <tr>
                                    <th><input id="row5" value="<%= data[1].id_conocimiento %>" type="number" class="form-control" disabled></th>
                                    <th><input id="row6" type="text" value="<%= data[1].descripcion_conocimiento %>" class="form-control" name='Frontend' disabled></th>
                                    <th><input id="row7" type="number" min="1" max="10" class="form-control" required></th>
                                    <th><input id="row8" type="number" min="1" max="10" class="form-control" required></th>
                                </tr>
                                <tr>
                                    <th><input id="row9" value="<%= data[2].id_conocimiento %>" type="number" class="form-control" disabled></th>
                                    <th><input id="row10" type="text" value='<%= data[2].descripcion_conocimiento %>' class="form-control" disabled></th>
                                    <th><input id="row11" type="number" min="1" max="10" class="form-control" required></th>
                                    <th><input id="row12" type="number" min="1" max="10" class="form-control" required></th>
                                </tr>
                                <tr>
                                    <th><input id="row13" value="<%= data[3].id_conocimiento %>" type="number" class="form-control" disabled></th>
                                    <th><input id="row14" type="text" value='<%= data[3].descripcion_conocimiento %>' class="form-control" disabled></th>
                                    <th><input id="row15" type="number" min="1" max="10" class="form-control" required></th>
                                    <th><input id="row16" type="number" min="1" max="10" class="form-control" required></th>
                                </tr>
                                <tr>
                                    <th><input id="row17" value="<%= data[4].id_conocimiento %>" type="number" class="form-control" disabled></th>
                                    <th><input id="row18" type="text" value='<%= data[4].descripcion_conocimiento %>' class="form-control" disabled></th>
                                    <th><input id="row19" type="number" min="1" max="10" class="form-control" required></th>
                                    <th><input id="row20" type="number" min="1" max="10" class="form-control" required></th>
                                </tr>
                            </tbody>
                            <tfoot class="table-active">
                                <tr>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td>
                                        <button type="submit" class="btn btn-success" data-toggle="modal">
                                                <i class="far fa-arrow-alt-circle-right"></i> Enviar datos 
                                        </button>
                                    </td>
                                </tr>
                            </tfoot>
                        </table>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <script src="/js/tablaconocimientos.js"></script>
    </body>

    </html>