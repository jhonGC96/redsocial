<%- include ("cabeceras/cabecera.principal.ejs") %>
    <div class="container p-4">
        <div class="row">
            <div class="col-md-12">
                <div class="table-wrapper col-md-12">
                    <div class="table-title col-md-12">
                        <div>
                            <h2>Valora tu desempe;o del 1-10
                            </h2>
                        </div>
                    </div>
                    <form id='tablaAlta' Method="post">
                        <table class="table table-striped table-hover col-md-12">
                            <thead class="thead-dark">
                                <tr>
                                    <th>No</th>
                                    <th>
                                        Desempe;os
                                    </th>
                                    <th>Antes de Tecla</th>
                                    <th>Despues de Tecla</th>
                                </tr>
                            </thead>

                            <tbody>
                                <tr>
                                    <th><input id="row1" value="<%= data[0].id_desempeno %>" type="number" class="form-control" disabled></th>
                                    <th><input id="row2" type="text" value="<%= data[0].descripcion_desempeno %>" class="form-control" disabled></th>
                                    <th><input id="row3" type="number" min="1" max="10" class="form-control" required></th>
                                    <th><input id="row4" type="number" min="1" max="10" class="form-control" required></th>
                                </tr>
                                <tr>
                                    <th><input id="row5" value="<%= data[1].id_desempeno %>" type="number" class="form-control" disabled></th>
                                    <th><input id="row6" type="text" value="<%= data[1].descripcion_desempeno %>" class="form-control" name='Frontend' disabled></th>
                                    <th><input id="row7" type="number" min="1" max="10" class="form-control" required></th>
                                    <th><input id="row8" type="number" min="1" max="10" class="form-control" required></th>
                                </tr>
                                <tr>
                                    <th><input id="row9" value="<%= data[2].id_desempeno %>" type="number" class="form-control" disabled></th>
                                    <th><input id="row10" type="text" value='<%= data[2].descripcion_desempeno %>' class="form-control" disabled></th>
                                    <th><input id="row11" type="number" min="1" max="10" class="form-control" required></th>
                                    <th><input id="row12" type="number" min="1" max="10" class="form-control" required></th>
                                </tr>
                            </tbody>
                            <tfoot class="table-active">
                                <tr>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td>
                                        <button type="submit" class="btn btn-success" data-toggle="modal">
                                                <i class="far fa-arrow-alt-circle-right"></i> Enviar datos 
                                        </button>
                                    </td>
                                </tr>
                            </tfoot>
                        </table>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <script src="/js/tabladesempeno.js"></script>

    </body>

    </html>